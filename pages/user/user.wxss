page {
  background-color: #eceff8;
  width: 100%;
  height: 100%;
}
.user_info_wrap {
  height: 30vh;
  /* background-color: #94c8f3; */
  background-image: url(https://z3.ax1x.com/2021/11/14/IcEp79.png);
}
.user_info_wrap .userHead {
  display: flex;
  position: relative;
}
.user_info_wrap .userHead .userIcon {
  padding: 30rpx;
}
.user_info_wrap .userHead .userIcon .image {
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  border: 4rpx solid white;
  background-color: yellow;
}
.user_info_wrap .userHead .userInfo {
  position: absolute;
  left: 174rpx;
  top: 43rpx;
}
.user_info_wrap .userHead .userInfo .userName {
  font-weight: 600;
}
.user_info_wrap .userHead .userInfo .identify {
  font-size: 26rpx;
}
.user_info_wrap .userHead .userEdit {
  width: 195rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 10px;
  border: 1px solid black;
  position: absolute;
  left: 512rpx;
  top: 70rpx;
  font-size: 24rpx;
}
.user_info_wrap .stateWrap {
  display: flex;
}
.user_info_wrap .stateWrap navigator {
  flex: 1;
  text-align: center;
}
.user_info_wrap .stateWrap navigator .number {
  font-weight: 600;
}
.user_info_wrap .stateWrap navigator .stateName {
  font-size: 28rpx;
  color: #888686;
}
.optionsWrap {
  border-radius: 15px;
  width: 93%;
  margin-top: 20rpx;
  margin-left: 27rpx;
  background-color: white;
}
.optionsWrap .Options {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 16rpx;
  border-bottom: 1px solid black;
}
.optionsWrap .Options .jiantou {
  margin-left: 432rpx;
}
.optionsWrap .options {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
  border-bottom: 1px solid black;
}
.optionsWrap .options .jiantou {
  margin-left: 435rpx;
}
.optionsWrap .options .jianTou {
  margin-left: 398rpx;
}
